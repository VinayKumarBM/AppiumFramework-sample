<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Appium Test Suite" thread-count="2" parallel="tests">
	<listeners>
		<listener class-name="com.appium.listeners.ExtentReporterListener"></listener>
		<listener class-name="com.appium.listeners.TestListener"></listener>
	</listeners>
	<test name="IOS Tests">
		<classes>
			<class name="com.ios.test.WebBrowserTest" />
 			<class name="com.ios.test.UIKitCatalogTest" />
		</classes>
	</test> 
	
	<test name="Android Tests">
		<classes>
			<class name="com.android.test.GenralStoreTest" />
			<class name="com.android.test.CalculatorAppTest" />	
			<class name="com.android.test.WebBrowserTest" />	 
		</classes>
	</test> 
</suite> 
